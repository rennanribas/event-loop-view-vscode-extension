{
  "name": "event-loop-monitor",
  "displayName": "Event Loop Monitor",
  "description": "A VS Code extension to monitor Node.js event loop in real-time",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.startMonitoring"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.startMonitoring",
        "title": "Start Event Loop Monitor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "typescript": "^4.0.3",
    "tslint": "^5.20.1",
    "vscode-test": "^1.4.0"
  }
}
